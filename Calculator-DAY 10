def add(n1, n2):
    return n1 + n2
def sub(n1,n2):
    return n1-n2
def mul(n1,n2):
    return n1*n2
def divide(n1,n2):
    return (n1/n2)
operator={
    '+': add,
     '-':sub,
    '*':mul,
     '/':divide}
f_no=int(input("enter the first number"))
work=input("Enter the symbol which u wanna perform")
s_no=int(input("Enter the second number"))
if work in operator:
    result=operator[work](f_no,s_no)
    print(f"The result is {result}")
else:
    print("Invalid operator")



